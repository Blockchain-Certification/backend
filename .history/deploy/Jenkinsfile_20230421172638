    agent any
    
    stages {
        stage('Deploy') {
           
            sshagent(['vinhphan-root-keys']) {
    // some block
}
        }
    }
}


pipeline {
    agent any
    stages {
        stage('Deploy') {
            steps {
                sshagent(credentials: ['vinhphan-root-keys']) {
                    sh 'ssh user@host "command"'
                }
            }
        }
    }
}

